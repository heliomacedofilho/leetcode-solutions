   "source": [
    "class Solution:\n",
    "    def isSubsequence(self, s: str, t: str) -> bool:\n",
    "        i = 0\n",
    "        j = 0\n",
    "        size_t = len(t)\n",
    "        size_s = len(s)\n",
    "        while (i < size_s and j < size_t):\n",
    "            if s[i] == t[j]:\n",
    "                i+=1\n",
    "            j+=1\n",
    "        return i == len(s)"
   ]
  }
