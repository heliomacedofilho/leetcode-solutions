   "source": [
    "class Solution:\n",
    "    def subtractProductAndSum(self, n: int) -> int:\n",
    "        p, s = 1, 0\n",
    "        while n > 0:\n",
    "            i = n % 10\n",
    "            p*=i\n",
    "            s+=i\n",
    "            n//=10\n",
    "        return p-s"
   ]
  }
