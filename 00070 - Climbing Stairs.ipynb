   "source": [
    "class Solution:\n",
    "    def climbStairs(self, n: int) -> int:\n",
    "        answer = [1, 1]\n",
    "        for i in range(2, n+1):\n",
    "            answer[i&1] = answer[0]+answer[1]\n",
    "            \n",
    "        return answer[n&1]"
   ]
  }
