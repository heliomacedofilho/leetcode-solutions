   "source": [
    "class Solution:\n",
    "    def countPrefixes(self, words: List[str], s: str) -> int:\n",
    "        \n",
    "        answer = 0\n",
    "        for word in words:\n",
    "            if s[:len(word)] == word:\n",
    "                answer += 1\n",
    "\n",
    "        return answer"
   ]
  }
