   "source": [
    "class Solution:\n",
    "    def maxProfit(self, prices: List[int]) -> int:\n",
    "        n = len(prices)\n",
    "        answer = 0\n",
    "        for i in range(1, n):\n",
    "            diff = prices[i] - prices[i-1]\n",
    "            answer += diff if diff > 0 else 0\n",
    "        return answer"
   ]
  }
