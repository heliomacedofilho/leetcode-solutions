   "source": [
    "class Solution:\n",
    "    def canMakeArithmeticProgression(self, arr: List[int]) -> bool:\n",
    "        arr.sort()\n",
    "        pa = arr[1] - arr[0]\n",
    "        for i in range(1, len(arr)-1):\n",
    "            if arr[i+1] - arr[i] != pa:\n",
    "                return False\n",
    "        return True"
   ]
  }
