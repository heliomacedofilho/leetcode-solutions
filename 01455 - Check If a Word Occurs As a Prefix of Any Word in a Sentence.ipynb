   "source": [
    "class Solution:\n",
    "    def isPrefixOfWord(self, sentence: str, searchWord: str) -> int:\n",
    "        for idx, word in enumerate(sentence.split(' '), 1):\n",
    "            if word.startswith(searchWord):\n",
    "                return idx\n",
    "        return -1"
   ]
  }
