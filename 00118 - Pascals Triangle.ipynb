    "class Solution:\n",
    "    def generate(self, numRows: int) -> List[List[int]]:\n",
    "        answer = [[1]]\n",
    "        for i in range(1, numRows):\n",
    "            answer.append([(answer[i-1][j-1] if j > 0 else 0) + (answer[i-1][j] if j < i else 0) for j in range(i+1)])\n",
    "        return answer"
   ]
  }
