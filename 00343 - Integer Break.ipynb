   "source": [
    "class Solution:\n",
    "    def integerBreak(self, n: int) -> int:\n",
    "        if n<4: return n-1\n",
    "        if n==4: return n\n",
    "        answer = 1\n",
    "        while n > 4:\n",
    "            answer *= 3\n",
    "            n -= 3\n",
    "        \n",
    "        return answer * n"
   ]
  }
