    "class Solution:\n",
    "    def rob(self, nums: List[int]) -> int:\n",
    "        \n",
    "        dp1, dp2 = 0, 0\n",
    "        \n",
    "        for i in range(len(nums)):\n",
    "            dp2, dp1 = max(nums[i] + dp1, dp2), dp2\n",
    "            \n",
    "        return dp2\n",
    "        "
   ]
